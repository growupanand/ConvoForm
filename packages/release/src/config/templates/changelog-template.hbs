

export const releases = [ 
  {{#each releases}}
  {
    "version": "{{title}}",
    "isoDate": "{{isoDate}}",
    "commits": [
          {{#commit-list commits heading="" message="feat: "}}
             { "type": "features",
                "message": "{{jsonSafeString message}}",
                "href": "{{href}}"
             }{{#unless @last}},{{/unless}}
            
          {{/commit-list}}
         
         {{#commit-list merges heading="" message="feat: "}}
            {
               "type": "features",
               "message": "{{jsonSafeString message}}",
               "href": "{{href}}"
            }{{#unless @last}},{{/unless}}
            
          {{/commit-list}}

          {{#commit-list commits heading="" subject="(refactor|style|perf): "}}
             {
               "type" : "improvements",
                "message": "{{jsonSafeString message}}",
                "href": "{{href}}"
             }{{#unless @last}},{{/unless}}

          {{/commit-list}}

           {{#commit-list merges heading="" subject="(refactor|style|perf): "}}
             {
               "type" : "improvements",
                "message": "{{jsonSafeString message}}",
                "href": "{{href}}"
             }{{#unless @last}},{{/unless}}

          {{/commit-list}}
 
          {{#commit-list commits heading="" subject="fix: "}}
              {
               "type" : "fixes",
                "message": "{{jsonSafeString message}}",
                "href": "{{href}}"
             }{{#unless @last}},{{/unless}}

          {{/commit-list}}

           {{#commit-list merges heading="" subject="fix: "}}
              {
               "type" : "fixes",
                "message": "{{jsonSafeString message}}",
                "href": "{{href}}"
             }{{#unless @last}},{{/unless}}

          {{/commit-list}}
      ]
    
  }{{#unless @last}},{{/unless}}
  {{/each}}
]