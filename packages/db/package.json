{
  "name": "@convoform/db",
  "version": "0.0.0",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./src/env": "./src/env.ts",
    "./src/envSchema": "./src/envSchema.ts",
    "./src/schema": "./src/schema/index.ts",
    "./src/*": "./src/*.ts"
  },
  "scripts": {
    "dev": "drizzle-kit studio --config drizzle.config.ts",
    "dev:turbo": "drizzle-kit studio --config drizzle.config.ts",
    "drizzle-kit": "drizzle-kit",
    "drizzle:generate-migration": "drizzle-kit generate",
    "drizzle:apply-migration": "drizzle-kit migrate",
    "drizzle:push": "drizzle-kit push",
    "custom-script": "tsx ./src/script.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "@paralleldrive/cuid2": "2.2.2",
    "@t3-oss/env-core": "0.13.10",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "0.8.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@convoform/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/pg": "^8.11.6",
    "drizzle-kit": "^0.30.2",
    "pg": "8.18.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "catalog:"
  }
}